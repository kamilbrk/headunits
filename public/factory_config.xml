    <!-- System settings -->
    <setings>
        <!-- Package name of default navigation application -->
        <NaviApp>com.zjinnova.zlink</NaviApp>

        <!--
            Image of rearview camera
            0：Not mirrored
            1：Mirrored
        -->
        <RearCamMirror>0</RearCamMirror>

        <!--
            Video disabled while driving
            0：Off
            1：On
        -->
        <DoNotPlayVideosWhileDriving>0</DoNotPlayVideosWhileDriving>

        <!-- Display track when reversing  0：Off  1：On -->
        <ShowTrack>1</ShowTrack>

        <!-- Display PDC when reversing  0：Off 1：On -->
        <ShowRadar>1</ShowRadar>

        <!-- Mute sound while reversing  0：Off 1：On -->
        <ReversingMuteSelect>0</ReversingMuteSelect>

        <!-- Open the front view camera  0：Off  1：On  -->
        <Front_view_camera>0</Front_view_camera>

        <!--
            Selection of reversing camera
            0：Aftermarket Rear camera
            1：Original car camera
            2：Aftermarket 360 camera
        -->
        <RearCamType>1</RearCamType>

        <!-- Brightness 0～100 -->
        <Brightness>70</Brightness>

        <!-- Android media volume 0～40 -->
        <Android_media_vol>26</Android_media_vol>

        <!-- Android phone volume 0～40 -->
        <Android_phone_vol>28</Android_phone_vol>

        <!-- Car phone volume 0～40 -->
        <Car_phone_vol>20</Car_phone_vol>

        <!-- Car navigation volume 0～40 -->
        <Car_navi_vol>20</Car_navi_vol>

        <!-- System language - see List of supported languages -->
        <Language>2</Language>

        <!-- Time synchronization source  0：Android time  1：Car system time -->
        <TimeSyncSoucrce>0</TimeSyncSoucrce>

        <!-- Time Format  0：24 hour  1：12 hour -->
        <TimeFormat>0</TimeFormat>

        <!-- Radio mix to navigation  0：shut down  1：30% 2：40% 3：50% 4：60%  5：70% -->
        <NaviMix>3</NaviMix>

        <!-- Equalizer  0:customize 1:Pop 2:Classic 3:Rock 4:Jazz 5:Dance -->
        <EQ_mode>0</EQ_mode>

        <!-- Bass  0~24 :  0 =>-12  12=>0  24=>+12 -->
        <Bass_value>12</Bass_value>

        <!-- Mid  0~24 :  0 =>-12  12=>0  24=>+12 -->
        <Middle_value>12</Middle_value>

        <!-- Treble  0~24 :  0 =>-12  12=>0  24=>+12 -->
        <Treble_value>12</Treble_value>

        <!-- Setting the AUX input of the car  0~12 :  0:switch off the AUX input -->
        <CarAuxIndex>2</CarAuxIndex>

        <!-- Temperature unit ：0：Celsius  1：Fahrenheit  -->
        <TempUnit>0</TempUnit>

        <!-- Fuel unit: 0：L  1：gal(US)  1：gal(UK) -->
        <FuelUnit>0</FuelUnit>
    </setings>

    <!-- Factory setting -->
    <factory>
        <!-- Version information-->
        <ver>ALS-BM-20221126.en</ver>

        <!--
            Vendor information possible values:
            - GS
            - KSW
            - ALS_6208
            - yc_2306
            - LC3208
            - CUSP_210407 (req 3.0.4)
        -->
        <client>ALS_6208</client>

        <!-- Password of factory setting (4~6 digits) -->
        <password>0000</password>

        <!--
            Theme selection for Android 7-8 firmwares
            See SupportUIList and their `ui` attributes
        -->
        <UI>1</UI>

        <!--
            Theme selection for Android 9+ firmwares
            See SupportUIList and their `name` attributes
        -->
        <UI_type>BMW_EVO_ID7_V2</UI_type>

        <!--
            Original display style
            0：full screen display
            1：Half screen display, driving computer information on the right
        -->
        <CarVideoDisplayStyle>0</CarVideoDisplayStyle>

        <!--
            Left logo of Audi home page
            0: Do not show
            1: A4 (default)
            2: A4L2
            3: A4L3
            4: Q5
            5: Special logo
            6: A3
            7: A5
            8: A6
            9: Q3
            10: Q7
            11: General
        -->
        <Audi_Logo_Left>1</Audi_Logo_Left>

        <!--
            Right logo of Audi home page
            0: Do not show
            1: Display AUDI LOGO And high German navigation information
            2: Display only AUDI LOGO
            3: Display driving information
            4: Display music information
        -->
        <Audi_Logo_Right>1</Audi_Logo_Right>

        <!-- Google APP  0:Off  1:On -->
        <GoogleAPP>1</GoogleAPP>

        <!-- Sound Effect APP: 0: Hide 1: Show -->
        <EQ_app>1</EQ_app>

        <!-- Global Weather APP 0: Hide 1: Show -->
        <globalweather_app>1</globalweather_app>

        <!-- FM transmission support  0：Off  1：On  -->
        <Support_FM_Transmit>0</Support_FM_Transmit>

        <!-- Support dashboard  0：Off  1：On  -->
        <Support_dashboard>1</Support_dashboard>

        <!-- DVD module type  0:Off  1: DVD software 1 -->
        <DVD_Type>0</DVD_Type>

        <!-- AUX function switch  0:Off  1:On -->
        <AUX_Type>1</AUX_Type>

        <!-- DTV function switch  0:Off  1:On -->
        <DTV_Type>0</DTV_Type>

        <!--
            DVR module type
            0: Off
            1: CVBS DVR
            2: USB DVR (The following USB DVR application names can be set using USB, otherwise it cannot be customized)
        -->
        <DVR_Type>0</DVR_Type>

        <!--
            USB DVR application names
            Default: Ankai (Valid when the DVR module type is USB DVR)
            Old: com.anwensoft.cardvr
            New: com.ankai.cardvr
        -->
        <DVRApk_PackageName>com.ankai.cardvr</DVRApk_PackageName>

        <!-- Bluetooth function  0: Bluetooth of the Android Unit 1: Bluetooth of the car  -->
        <BT_Type>0</BT_Type>

        <!-- Installation of third-party apps 0:allow  1:Prohibited  -->
        <APK_Install>0</APK_Install>

        <!--
            Type of the original head unit with AUX input
            1: Alpine
            2: Harman
            3: other
        -->
        <CarAux_auto_method>2</CarAux_auto_method>

        <!--
            Switching mode of original head unit AUX input
            0: Automatic switching
            1: Manual switching
        -->
        <CarAux_Operate>0</CarAux_Operate>

        <!--
            Amplifier selection:
            0: Original car amplifier
            1: External amplifier box
        -->
        <AMP_Type>0</AMP_Type>

        <!--
            Old BMW X1 (no display)
            0: Display clock
            1: Show original car monitor
        -->
        <CarDisplay>1</CarDisplay>

        <!-- Support for voice commands  0:Off  1:On -->
        <Support_TXZ>0</Support_TXZ>

        <!-- Awakening words -->
        <TXZ_Wakeup>Hello, Serena</TXZ_Wakeup>

        <!-- Support for voice wake-up  0:Off  1:On -->
        <Support_TXZ_Voice_Wakeup>0</Support_TXZ_Voice_Wakeup>

